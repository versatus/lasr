        },
        "token": {
          "$ref": "#/definitions/Address"
        },
        "token_ids": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/U256"
          }
        }
      }
    },
    "ContractLogType": {
      "oneOf": [
        {
          "type": "object",
          "required": [
            "Info"
          ],
          "properties": {
            "Info": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "Error"
          ],
          "properties": {
            "Error": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "Warn"
          ],
          "properties": {
            "Warn": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "Debug"
          ],
          "properties": {
            "Debug": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      ]
    },
    "CreateInstruction": {
      "type": "object",
      "required": [
        "distribution",
        "initialized_supply",
        "program_id",
        "program_namespace",
        "program_owner",
        "total_supply"
      ],
      "properties": {
        "distribution": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/TokenDistribution"
          }
