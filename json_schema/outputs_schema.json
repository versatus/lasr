                  "$ref": "#/definitions/U256"
                }
              ],
              "maxItems": 2,
              "minItems": 2
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "Extend"
          ],
          "properties": {
            "Extend": {
              "type": "array",
              "items": {
                "type": "array",
                "items": [
                  {
                    "$ref": "#/definitions/Address"
                  },
                  {
                    "$ref": "#/definitions/U256"
                  }
                ],
                "maxItems": 2,
                "minItems": 2
              }
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "Remove"
          ],
          "properties": {
            "Remove": {
              "type": "array",
              "items": [
                {
                  "$ref": "#/definitions/Address"
                },
                {
                  "$ref": "#/definitions/U256"
                }
              ],
              "maxItems": 2,
              "minItems": 2
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "Revoke"
          ],
          "properties": {
            "Revoke": {
              "$ref": "#/definitions/Address"
            }
          },
          "additionalProperties": false
        }
      ]
    },
    "ApprovalsValue": {
      "oneOf": [
